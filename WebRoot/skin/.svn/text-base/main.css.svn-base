.btn,
.btn input,
.btn span,
.btn-1,
.btn-1 span,
.btn-1 input,
.btn-2,
.btn-2 input{background:url(images/btn-1.png) no-repeat;display:inline-block;padding-left:10px;border:0;}
.btn span,
.btn input,
.btn-1 span,
.btn-1 input,
.btn-2 input{background-position:right 0;height:33px;line-height:33px;padding:0 18px 0 8px;font-size:14px;color:#fff;cursor:pointer;}
.btn:hover{background-position:0 -37px;}
.btn:hover span,
.btn:hover input{background-position:right -37px;}
.btn:active{background-position:0 -75px;}
.btn:active span,
.btn:active input{background-position:right -75px;}
.btn-1{background-position:0 -113px;}
.btn-1 span,
.btn-1 input{background-position:right -113px;color:#3a3a3a;}
.btn-1:hover{background-position:0 -152px;}
.btn-1:hover span,
.btn-1:hover input{background-position:right -152px;}
.btn-1:active{background-position:0 -190px;}
.btn-1:active span,
.btn-1:active input{background-position:right -190px;}
.btn-2{background-position:0 -228px;}
.btn-2 input{background-position:right -228px;color:#3a3a3a;padding:0 10px 0 0;margin-left:0;margin-right:0}
.btn input{background-position:right 0;color:#3a3a3a;padding:0 10px 0 0;margin:0}
.btn-2 #seachBtn{margin:0}
.btn-2 #uploadFMsubmit{margin:0}
.btn-2 #cancelUploadbox{margin:0}
.btn-2 #createBtn{margin:0}
.btn-2 #editconfirmBtn{margin:0}
