.btn-arrowL{background-position:-86px 0;left:0;}
.btn-arrowL:hover{background-position:-118px 0;}
.btn-high{width:117px;height:23px;background-position:0 -518px;text-align:center;padding:0 7px 0 0;}

/* form */
.in{background:url(images/bg-5.png) repeat-x;border:1px solid #dadada;width:330px;padding:0 5px;height:33px;line-height:33px;color:#656465;}
.in:hover{border-color:#e58d91;}
.in1{background:#fff;border:1px solid #dadada;height:24px;line-height:24px;padding:0 5px;color:#656465;}
.in2{background:#fff;border:1px solid #dadada;height:20px;line-height:20px;padding:0 5px;color:#656465;}
.in3{margin-top:-3px;}
select{background:#fff;border:1px solid #dadada;height:24px;line-height:20px;padding:2px 0;}
select option{padding:0 10px;}
textarea{background:#fff;border:1px solid #dadada;height:60px;width:82%;padding:0 5px;}

body{word-spacing:4px;}
#main .inner{padding:15px;}
.mR20{margin-right:20px;}
.breadCrumb{word-spacing:5px;margin:0 0 10px;}
.x{font-family:"宋体";color:#dd0d0b;}
.bti{background:#f8f8f8;border:1px solid #dadada;padding:4px 15px;font-size:14px;font-weight:normal;margin:0 0 10px;}
.bti1{font-size:14px;margin-bottom:10px;font-weight:normal;}
.bti2{font:20px/25px "Microsoft Yahei";color:#dd0d0b;border-bottom:1px solid #dadada;padding:0 0 10px;margin:0 0 10px;}
.bti2 h1{font:18px/25px "Microsoft Yahei";color:#dd0d0b;display:inline;margin:0 20px 0 0;}

/* search */
.search{height:36px;margin-bottom:10px;position:relative;}
.search .in{margin-left:5px;}
.search .ico-adds{position:absolute;top:-12px;right:0;}

/* tree */
.ico-plus,
.ico-minus{background:url(images/ico-4.png) no-repeat;display:block;overflow:hidden;float:left;cursor:pointer;margin:5px 5px 0;width:20px;height:11px;float:left;display:block;}
.ico-plus{background-position:0 -154px;}
.ico-minus{background-position:0 -103px;height:16px;}
.ico-blank{display:block;float:left;width:6px;height:21px;}
.tree .tHead{font-weight:bold;margin:20px 0 0 5px;height:21px;}
.tree .tNode{line-height:21px;height:21px;}
.tree .tBif,
.tree .tEnd{background:url(images/ico-4.png) no-repeat 5px -281px;display:block;float:left;width:20px;height:21px;}
.tree .tBif{margin:0 0 0 5px;}
.tree .tEnd{height:11px;overflow:hidden;background-position:10px -281px;width:25px;}
.tree .tLine{background:url(images/bg-7.png) repeat-y 10px 0;width:25px;display:block;float:left;height:21px;}
.tree .tEmpty{width:25px;display:block;float:left;height:21px;}


/* table */
.table{border:1px solid #dcdcdc;border-collapse:collapse;width:100%;margin:0 0 10px;}
.table td,
.table th{border:1px solid #dcdcdc;padding:5px 3px;}
.table th{background:#fafafa;font-weight:normal;}
.table td{color:#3a3a3a;text-align:center;}
.table .tr1 td{background:#fafafa;}
.table .tr2 td{background:#f0f0f0;}
.table .tr3 td{color:#999;background:#f4f4f4;}
.table .tr4 td{background:#fcfcfc;}
.table .t1 a,
.table td a{margin-right:6px;}
.table .tr3 .t2{background:#fff;}

.table1 td{padding:2px 5px;}

.table2{border-collapse:collapse;}
.table2 td{padding:2px 5px;}

/* page */
.page .ym{margin-right:20px;}
.page .ym a{display:inline-block;padding:3px 9px;background:#fcfcfc;border:1px solid #dadada;vertical-align:top;cursor:pointer;margin:0 2px;}
.page .ym a:hover{background:#e7e7e7;color:#3a3a3a;text-decoration:none;}
.page .home,
.page .pre,
.page .next,
.page .end{width:10px;height:18px;vertical-align:top;}
.page .home{background-position:-52px -125px;}
.page .pre{background-position:2px -125px;}
.page .next{background-position:3px -157px;}
.page .end{background-position:-52px -157px;}
.page .disable{cursor:default;}
.page .ym .disable:hover{background:none;}
.page .disable em{opacity:0.4;alpha:filter(opacity=40);cursor:default;}
.page .ym .current{background:#bdbdbd;border-color:#afafaf;color:#fff;font-weight:bold;}
.page .in{width:40px;height:25px;line-height:25px;margin-right:5px;}
.page .btn-go1{margin-left:5px;}

/* tab */
.tab{border-left:1px solid #dadada;margin-bottom:-1px;margin-top:5px;}
.tab li{float:left;background:#fcfcfc;line-height:25px;padding:0 20px;display:inline;cursor:pointer;border:1px solid #dadada;border-bottom:0;border-left:0;color:#333;}
.tab .active{height:26px;position:relative;background:#fff;border-top-color:#dd0d0b;font-weight:bold;color:#000;}
.tabTxt{margin-top:-1px;min-height:18px;_height:18px;border-top:1px solid #dadada;padding-top:10px;}

